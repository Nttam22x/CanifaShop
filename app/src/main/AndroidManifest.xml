<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.canifa_shop">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Canifa_Shop">
        <activity android:name=".Manager.WarehouseReportActivity"></activity>
        <activity android:name=".Manager.ImportProductActivity" />
        <activity android:name=".Bill.BillDetailActivity" />
        <activity android:name=".Emplyee.EmployeeManagerActivity" />
        <activity android:name=".Account.AcountManagerActivity" />
        <activity android:name=".Manager.WarehouseManagerActivity" />
        <activity android:name=".Customer.CustomerDetailActivity" />
        <activity android:name=".Customer.CustomerActivity" />
        <activity android:name=".Category.CategoryDetail" />
        <activity android:name=".Category.CategoryActivity" />
        <activity android:name=".Product.ProductDetailActivity" />
        <activity android:name=".Product.ProductActivity" />
        <activity android:name=".Login.LoginActivity">

        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>